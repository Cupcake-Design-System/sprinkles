{
  "name": "@ipreo/ngx-sprinkles",
  "scripts": {
    "ng": "ng",
    "start": "npm run storybook",
    "build": "ng build",
    "snapshot": "build-storybook && percy-storybook --widths=1400",
    "test": "jest",
    "test:o": "jest --json --outputFile=.jest-test-results.json",
    "test:ow": "jest --json --outputFile=.jest-test-results.json --watch",
    "lint": "ng lint",
    "storybook": "start-storybook -p 6006",
    "prebuild:storybook": "npm run test:generate-output",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "build:lib": "ng-packagr -p src/package.json",
    "build:theme": "node theme.build.js",
    "deploy-storybook": "storybook-to-ghpages",
    "test:ci:sonar": "jest --collect-coverage"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "testResultsProcessor": "jest-sonar-reporter",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ]
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://code.ipreo.com/Ipreo/sprinkles"
  },
  "peerDependencies": {
    "ng-click-outside": "^4.0.0",
    "fastdom": "^1.0.9"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^9.0.0-rc.4",
    "@angular/cli": "~7.3.5",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ant-design/icons-angular": "^9.0.0-rc.4",
    "@babel/core": "^7.3.4",
    "@cupcake-ds/cupcake": "^2.0.1",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@percy/storybook": "^3.2.0",
    "@storybook/addon-a11y": "^5.2.6",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-backgrounds": "^5.2.6",
    "@storybook/addon-cssresources": "^5.2.6",
    "@storybook/addon-jest": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-notes": "^5.2.6",
    "@storybook/addon-viewport": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/angular": "^5.2.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/jest": "^24.0.11",
    "@types/node": "~8.9.4",
    "@types/storybook__react": "^4.0.1",
    "babel-loader": "^8.0.5",
    "codelyzer": "~4.5.0",
    "concat": "^1.0.3",
    "core-js": "^2.5.4",
    "date-fns": "^1.30.1",
    "fastdom": "^1.0.9",
    "find": "^0.3.0",
    "jest": "^24.5.0",
    "jest-preset-angular": "^7.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "ng-click-outside": "^4.0.0",
    "ng-packagr": "^4.7.1",
    "protractor": "~5.4.0",
    "rxjs": "~6.3.3",
    "storybook-dark-mode": "^0.1.3",
    "ts-node": "~7.0.0",
    "tsickle": "^0.34.3",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "zone.js": "~0.8.26"
  },
  "dependencies": {
    "@angular/core": "^8.2.14"
  }
